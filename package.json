{
  "name": "@jc-jacobe/react-kit",
  "version": "1.0.2",
  "description": "A CLI Tool to Create a React + Vite Pre-Setup Application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "react-kit": "dist/index.js"
  },
  "files": [
    "dist",
    "template"
  ],
  "scripts": {
    "build": "tsc && npm run copy-template",
    "copy-template": "cpx \"template/**\" dist/template",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "react",
    "vite",
    "template",
    "cli",
    "starter"
  ],
  "author": "John Carlo Pura Jacobe",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.0",
    "cpx2": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
